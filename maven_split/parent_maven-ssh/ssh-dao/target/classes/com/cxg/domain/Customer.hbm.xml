<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cxg.domain">
	<class name="Customer" table="cst_customer">
		<id name="custId" type="java.lang.String">
			<column name="id" length="32"/>
			<generator class="uuid" />
		</id>

		<property name="custName" type="java.lang.String">			
		</property>
		<property name="custPhone" type="java.lang.String">			
		</property>
		<property name="custMobile" type="java.lang.String">			
		</property>
	</class>
</hibernate-mapping>
